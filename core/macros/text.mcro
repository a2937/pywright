macro cmode
setprop name=$_speaking prop=mode $1
setprop name=$_speaking prop=supermode $1
endmacro

macro blink
cmode blink
endmacro

macro talk
cmode talk
endmacro

macro lipsync
cmode lipsync
endmacro

macro loop
{cmode loop}
endmacro


macro linked
joinvar _return {spd0} $_last_written_text {spd1}
endmacro

